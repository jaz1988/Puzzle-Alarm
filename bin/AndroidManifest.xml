<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jyalarm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.jyalarm.AlarmActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.worldsproject.puzzle.PuzzleSolveActivity" >
        </activity>
        <activity android:name="org.worldsproject.puzzle.PuzzleSelectActivity" >
        </activity>
        <activity
            android:name="com.example.jyalarm.MainActivity"
            android:label="@string/title_activity_alarm" >
        </activity>
        
        <activity
            android:name="com.example.jyalarm.AlarmReceiverActivity"
            android:label="@string/title_activity_alarm" >
        </activity>
    </application>

</manifest>